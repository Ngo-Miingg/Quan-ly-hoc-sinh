@model Student_Management.Models.LoginViewModel

<h2>Đăng nhập</h2>
<form asp-action="Login" method="post">
    <div class="form-group">
        <label asp-for="TenDangNhap"></label>
        <input asp-for="TenDangNhap" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="MatKhau"></label>
        <input asp-for="MatKhau" class="form-control" type="password" />
    </div>
    <button type="submit" class="btn btn-primary">Đăng nhập</button>
</form>
